{
  "name": "life-dash-backend",
  "version": "2.4.0",
  "author": "",
  "license": "ISC",
  "description": "A framework for building frontend web projects",
  "main": "app.js",
  "scripts": {
    "test": "echo Mocha tests to be added...",
    "start": "node app.js",
    "start:dev": "nodemon app.js",
    "lint": "eslint --ext=js server",
    "lint:fix": "eslint --ext=js --fix server",
    "debug": "node app.js --inspect --debug-brk=5858"
  },
  "dependencies": {
    "app-module-path": "^2.2.0",
    "body-parser": "^1.20.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
    "helmet": "^3.22.0",
    "moment": "^2.24.0",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "nodemon": "^2.0.2"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "next"
        }
      ],
      "node/no-missing-require": [
        "error",
        {
          "allowModules": [
            "log"
          ]
        }
      ]
    }
  }
}
